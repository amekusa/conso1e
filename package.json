{
  "name": "conso1e",
  "version": "1.2.0",
  "description": "console wrapper + extra features",
  "files": [
    "src",
    "bundle.js",
    "bundle.js.map"
  ],
  "exports": {
    "import": "./src/Logger.js",
    "require": "./bundle.js"
  },
  "scripts": {
    "prepublishOnly": "npm run test && npm run build",
    "setup": "node setup.js",
    "watch": "npm-watch",
    "test": "nyc mocha",
    "build": "rollup src/Logger.js --file bundle.js --sourcemap --format umd --name conso1e",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "_setup": {
    "deps": {
      "rollup": {
        "version": "2.23.0"
      },
      "mocha": {
        "version": "8.1.1"
      },
      "nyc": {
        "version": "15.1.0"
      },
      "codecov": {
        "version": "3.7.2"
      },
      "npm-watch": {
        "version": "0.6.0"
      }
    }
  },
  "watch": {
    "test": {
      "patterns": [
        "test.js",
        "bundle.js"
      ],
      "inherit": true
    },
    "build": {
      "patterns": "src/*.js",
      "inherit": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amekusa/conso1e.git"
  },
  "keywords": [
    "console",
    "suppress",
    "buffer"
  ],
  "author": "amekusa (https://amekusa.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amekusa/conso1e/issues"
  },
  "homepage": "https://github.com/amekusa/conso1e#readme"
}
