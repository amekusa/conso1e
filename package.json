{
  "name": "conso1e",
  "version": "1.2.2",
  "description": "console wrapper + extra features",
  "files": [
    "src",
    "bundle.js",
    "bundle.js.map"
  ],
  "exports": {
    "import": "./src/Logger.js",
    "require": "./bundle.js"
  },
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "setup": "node setup.js",
    "setup:dry": "node setup.js --dry-run",
    "watch": "npm-watch",
    "clean": "rm -f bundle.js bundle.js.map && find . -name '.DS_Store' -not -path '*/node_modules/*' | xargs rm -r",
    "build": "rollup -c",
    "test": "c8 mocha --enable-source-maps",
    "codecov": "c8 report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "_setup": {
    "deps": {
      "npm-watch": "^0.11.0",
      "rollup": "^2.56.2",
      "c8": "^7.8.0",
      "codecov": "^3.8.3",
      "mocha": "^9.0.3"
    }
  },
  "watch": {
    "test": {
      "patterns": [
        "test.js",
        "bundle.js"
      ],
      "inherit": true
    },
    "build": {
      "patterns": "src/*.js",
      "inherit": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amekusa/conso1e.git"
  },
  "keywords": [
    "console",
    "suppress",
    "buffer"
  ],
  "author": "amekusa (https://amekusa.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amekusa/conso1e/issues"
  },
  "homepage": "https://github.com/amekusa/conso1e#readme"
}
